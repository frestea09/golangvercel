{
  "version": 3,
  "builds": [
    {
      "src": "api/index.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/items",

      "dest": "main.go"
    },

    {
      "src": "/item",

      "dest": "main.go"
    },

    {
      "src": "/item/create",

      "dest": "main.go"
    },

    {
      "src": "/item/update",

      "dest": "main.go"
    },

    {
      "src": "/item/delete",

      "dest": "main.go"
    }
  ]
}
